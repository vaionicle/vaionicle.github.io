---
layout: none
sitemap: false
is_wiki_page: false
permalink: /manifest.json
---
{
  "name": "{{ site.name }}",
  "short_name": "{{ site.name }}",
  "description": "{{ site.description }}",
  "background_color": "#000000",
  "theme_color": "#cccccc",
	"display": "standalone",
	"icons": [
		{
			"src": "{{ '/assets/images/iconfinder_terminal_x128.png?v=' | append: site.github.build_revision | relative_url }}",
			"sizes": "128x128",
			"type": "image/png"
		},
		{
			"src": "{{ '/assets/images/iconfinder_terminal_x256.png?v=' | append: site.github.build_revision | relative_url }}",
			"sizes": "256x256",
			"type": "image/png"
		},
		{
			"src": "{{ '/assets/images/iconfinder_terminal_x512.png?v=' | append: site.github.build_revision | relative_url }}",
			"sizes": "512x512",
			"type": "image/png"
		}
	]
}