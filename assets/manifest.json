---
layout: none
sitemap: false
is_wiki_page: false
permalink: /manifest.json
---
{
  "name": "{{ site.name }}",
  "short_name": "{{ site.name }}",
  "description": "{{ site.description }}",
  "background_color": "#000000",
  "theme_color": "#cccccc",
	"display": "standalone",
	"icons": [
		{"sizes": "36x36", "type": "image/png", "src": "{{ '/assets/images/command_prompt/android-36x36.png?v=' | append: site.github.build_revision | relative_url }}"},
		{"sizes": "48x48", "type": "image/png", "src": "{{ '/assets/images/command_prompt/android-48x48.png?v=' | append: site.github.build_revision | relative_url }}"},
		{"sizes": "72x72", "type": "image/png", "src": "{{ '/assets/images/command_prompt/android-72x72.png?v=' | append: site.github.build_revision | relative_url }}"},
		{"sizes": "96x96", "type": "image/png", "src": "{{ '/assets/images/command_prompt/android-96x96.png?v=' | append: site.github.build_revision | relative_url }}"},
		{"sizes": "144x144", "type": "image/png", "src": "{{ '/assets/images/command_prompt/android-144x144.png?v=' | append: site.github.build_revision | relative_url }}"},
		{"sizes": "192x192", "type": "image/png", "src": "{{ '/assets/images/command_prompt/android-192x192.png?v=' | append: site.github.build_revision | relative_url }}"}
